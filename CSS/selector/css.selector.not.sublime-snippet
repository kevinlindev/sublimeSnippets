<snippet>
  <content><![CDATA[
// The :not(selector) selector matches every element that is NOT the specified element/selector.
:not(${1:selector}) {
  background: #ff0000;
}
]]></content>
  <!-- Optional: Set a tabTrigger to define how to trigger the snippet -->
  <tabTrigger>css.selector.not</tabTrigger>
  <!-- Optional: Set a scope to limit where the snippet will trigger -->
  <scope>source.css, source.sass, source.css.less, text.html, text.html.jsp, text.plain</scope>
</snippet>
