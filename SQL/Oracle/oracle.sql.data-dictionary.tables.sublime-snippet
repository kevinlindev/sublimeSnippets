-- Data Dictionary Table Info
DESC all_tables;
DESC dba_tables;
DESC user_tables;

-- Sample Query
SELECT
  *
FROM all_tables t
WHERE t.table_name LIKE 'ACT_%'
AND t.owner = 'BUILDING';
]]></content>
